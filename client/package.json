    "format": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "preview": "vite preview",
    "plop": "cross-env NODE_OPTIONS=--no-warnings plop",
    "plop:component": "cross-env NODE_OPTIONS=--no-warnings plop component",
    "plop:service": "cross-env NODE_OPTIONS=--no-warnings plop service"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router": "^7.8.0",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/mongoose": "^5.11.96",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "handlebars": "^4.7.8",
    "plop": "^4.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0"
