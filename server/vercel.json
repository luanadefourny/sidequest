    {
      "version": 2,
      "builds": [
        {
          "src": "api/index.ts",
          "use": "@vercel/node"
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "/api/index.ts",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
          "headers": {
            "Access-Control-Allow-Credentials": "true",
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Methods": "GET,PATCH,DELETE,POST,PUT",
            "Access-Control-Allow-Headers": "Content-Type"
          }
        }
      ]
    }
